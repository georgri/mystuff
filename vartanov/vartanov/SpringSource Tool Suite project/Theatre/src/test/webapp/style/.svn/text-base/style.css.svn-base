@CHARSET "UTF-8"; 
body {margin: 30px; font-size: 16px}
a {color: #666; text-decoration: underline}
a:hover {color: #fff; background: #666; text-decoration: none}
.number {color: #888; font-size: 10px} 
.title {font-size: 16px}
.g {color: #888; font-size: 10px}
.l {font-size: 50px; color: #4df}
table {cellpadding: 0px; cellspacing: 0px}
td {padding: 0px; margin: 0px; border: none}
.top {background-color: #000; color: #fff; padding: 10px; font-size: 25px}
.top a {color: #fff; text-decoration: none}
.bottom {background-color: #eee; color: #888; padding: 20px; font-size: 12px; float: none}
